Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa_amerenda_personal
  IdentityFile ~/.ssh/google_compute_engine
  IdentityFile ~/.ssh/id_rsa_impact

### Git servers
Host github.com
  IdentityFile ~/.ssh/github_deploy_impact

Host bitbucket.org
  IdentityFile ~/.ssh/github_deploy

#### Work Servers
## Prod
Host azkaban-prod
  Hostname azkaban-us-central1-a.int.2pth.com
  ProxyCommand ssh bastion -W %h:%p
  StrictHostKeyChecking no

Host bastion
  Hostname 35.199.17.108
  IdentityFile ~/.ssh/google_compute_engine
  ControlPath ~/.ssh/bastion-%r@%h:%p.ctl
  ControlMaster auto
  ControlPersist 10m

# Monitoring
Host prom
  Hostname prometheus-us-central1-a.int.2pth.com
  ProxyCommand ssh bastion -W %h:%p
  StrictHostKeyChecking no

## Staging
Host bastion-stg
  Hostname 35.199.63.95
  IdentityFile ~/.ssh/google_compute_engine
  ControlPath ~/.ssh/bastion-stg.ctl

Host prom-stg
  Hostname 10.25.48.13
  ProxyCommand ssh bastion -W %h:%p
  StrictHostKeyChecking no

